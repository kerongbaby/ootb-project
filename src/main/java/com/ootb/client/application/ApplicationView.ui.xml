<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<!-- <g:SimplePanel ui:field="main"/> -->
	<g:DockLayoutPanel height="500px" width="100%">
		<g:north size="50">
			<g:HorizontalPanel ui:field="blackTitle" width="100%"
				height="100%">
				<g:HTMLPanel>Welcome to Out-of-the-BOX</g:HTMLPanel>
				<g:Button ui:field="delivery">Delivery</g:Button>
				<g:Button ui:field="homeButton">Home</g:Button>
			</g:HorizontalPanel>
		</g:north>
		<g:center>
			<g:DockLayoutPanel ui:field="main" height="100%"
				width="100%"></g:DockLayoutPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
